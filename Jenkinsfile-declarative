@Library('jenkins-shared-library') _
import org.example.Utils

pipeline {
    agent any
    stages {
        stage('Greet') {
            steps {
                sayHello('Dimitry')
            }
        }
        stage('Shout') {
            steps {
                script {
                    def msg = Utils.shout("hello from declarative")
                    echo msg
                }
            }
        }
    }
}
